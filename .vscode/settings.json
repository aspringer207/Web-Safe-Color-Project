{
    "cSpell.words": [
        "Belanosima",
        "huebox"
    ],
    "prettier.bracketSameLine": false,
    "vs-html-to-css.ignore": [

        "container",
        "row"
    ],
    "editor.defaultFoldingRangeProvider": "vscode.html-language-features",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "css-auto-prefix.prefixes": {

        "transform": [
            "webkit",
            "moz",
            "ms",
            "o"
        ],
        "transition": [
            "webkit",
            "moz",
            "ms",
            "o"
        ],
        "border-radius": [
            "webkit",
            "moz",
            "ms",
            "o"
        ],
        "animation": [
            "webkit"
        ],
        "box-reflect": [
            "webkit"
        ],
        "filter": [
            "webkit"
        ],
        "font-feature-settings": [
            "webkit",
            "moz"
        ],
        "backface-visibility": [
            "webkit"
        ],
        "box-decoration-break": [
            "webkit"
        ],
        "hyphens": [
            "webkit",
            "moz",
            "ms"
        ],
        "marquee-direction": [
            "webkit"
        ],
        "marquee-play-count": [
            "webkit"
        ],
        "marquee-speed": [
            "webkit"
        ],
